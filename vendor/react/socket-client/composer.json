{
    "name": "react/socket-client",
    "description": "Async, streaming plaintext TCP/IP and secure TLS based connections for ReactPHP",
    "keywords": ["async", "socket", "stream", "connection", "ReactPHP"],
    "license": "MIT",
    "require": {
        "php": ">=5.3.0",
        "react/dns": "0.4.*|0.3.*",
        "react/event-loop": "0.4.*|0.3.*",
        "react/stream": "^0.6 || ^0.5 || ^0.4.5",
        "react/promise": "^2.1 || ^1.2",
        "react/promise-timer": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "React\\SocketClient\\": "src"
        }
    },
    "require-dev": {
        "clue/block-react": "^1.1",
        "react/socket": "^0.5",
        "phpunit/phpunit": "~4.8",
        "react/stream": "^0.6"
    }
}
